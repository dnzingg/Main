local function GetWorlds()
    local worlds = {}
    for _, v in ipairs(Workspace:GetChildren()) do
        if v:IsA("Folder") and v:FindFirstChild("SurfacePart") then table.insert(worlds, v.Name) end
    end
    if #worlds == 0 then table.insert(worlds, nil) end
    return worlds
end
local DropdownSelectWorldToTeleport = Teleports:CreateDropdown({
	Name = "Select World To Teleport",
	Options = GetWorlds(),
	CurrentOption = nil,
	Flag = "Select World To Teleport",
	Callback = function(Option)
	if type(Option) == "table" then Option = Option[1] end
        getgenv().currentSelectWorld = Option or nil
	end,
})

local ButtonTeleportToSelectedWorld = Teleports:CreateButton({
	Name = "Teleport To Selected World",
	Callback = function()
	if getgenv().currentSelectWorld ~= nil then
	for i,v in pairs(game.Workspace:GetChildren()) do
	if v.Name == getgenv().currentSelectWorld then
	hrp.CFrame = v.SurfacePart.CFrame + Vector3.new(0, 3.5, 0)
	end
	end
	end
	end,
})

local ToggleAutoWin = Main:CreateToggle({
	Name = "Auto Win",
	CurrentValue = false,
	Flag = "Auto Win",
	Callback = function(Value)
	getgenv().autoWin = Value
	
	if Value then
	hrp.CFrame = plrWorld.WinPart.CFrame + Vector3.new(0, 50, 0)
	end
	while autoWin do task.wait(10.5)
	hrp.CFrame = plrWorld.WinPart.CFrame + Vector3.new(0, 50, 0)
	end
	end,
})

local function GetPlayerPets()
	local pets, found = {}, {}
	local folder = plr.Pets
	if folder then
		for _, v in pairs(folder:GetChildren()) do
			if v:IsA("StringValue") and not found[v.Name] then
				found[v.Name] = true
				table.insert(pets, v.Name)
			end
		end
	end
	if #pets == 0 then table.insert(pets, nil) end
	return pets
end
local DropdownSelectPetToUpgrade = Shop:CreateDropdown({
	Name = "Select Pet To Upgrade",
	Options = GetPlayerPets(),
	CurrentOption = nil,
	Callback = function(Option)
	if type(Option) == "table" then Option = Option[1] end
        getgenv().currentSelectedPet = Option or nil
	end,
})

Shop:CreateButton({
	Name = "Refresh Pet List",
	Callback = function()
		DropdownSelectPetToUpgrade:Refresh(GetPlayerPets())
		SendNotification("GG hub", "Pet List have been refreshed!", 2)
	end
})

local DropdownSelectQuantityToUpgrade = Shop:CreateDropdown({
	Name = "Select Quantity To Upgrade",
	Options = {"1","2","3","4","5"},
	CurrentOption = "1",
	Flag = "Select Quantity To Upgrade",
	Callback = function(Option)
	if Option == "1" then
	getgenv().quantityPet = 20
	end
	if Option == "2" then
	getgenv().quantityPet = 40
	end
	if Option == "3" then
	getgenv().quantityPet = 60
	end
	if Option == "4" then
	getgenv().quantityPet = 80
	end
	if Option == "5" then
	getgenv().quantityPet = 100
	end
	end,
})

local AutoUpgradeSelectedPet = Shop:CreateToggle({
	Name = "Auto Upgrade Selected Pet",
	CurrentValue = false,
	Flag = "Auto Upgrade Selected Pet",
	Callback = function(Value)
	getgenv().autoUpgrade = Value
	
	if Value then
	for _,v in pairs(plr.Pets:GetChildren()) do
	if v.Name == getgenv().currentSelectedPet then
	local args = {
	tostring(v.Name),
	getgenv().quantityPet
}
game:GetService("ReplicatedStorage"):WaitForChild("PetRemotes"):WaitForChild("GoldPetCraftEvent"):FireServer(unpack(args))
	end
	end
	end
	if getgenv().currentSelectedPet ~= nil then
	while autoUpgrade do task.wait(3.5)
	for _,v in pairs(plr.Pets:GetChildren()) do
	if v.Name == getgenv().currentSelectedPet then
	local args = {
	tostring(v.Name),
	getgenv().quantityPet
}
game:GetService("ReplicatedStorage"):WaitForChild("PetRemotes"):WaitForChild("GoldPetCraftEvent"):FireServer(unpack(args))
	end
	end
	end
	else
	SendNotification("GG hub", "Select a pet to upgrade first!", 2)
	end
	end,
})

local DropdownSelectMethodToSpin = Shop:CreateDropdown({
	Name = "Select Method To Spin",
	Options = {
		"350 Gems", "250M Money", "One Spin", "Dominus Pet", "875 Gems",
		"100M Money", "3 Spins", "Random Pickaxe", "265 Gems", "10 Spins"
	},
	CurrentOption = nil,
	Flag = "Select Method To Spin",
	Callback = function(Option)
		if type(Option) == "table" then Option = Option[1] end
		local map = {
			["350 Gems"] = 1,
			["250M Money"] = 2,
			["One Spin"] = 3,
			["Dominus Pet"] = 4,
			["875 Gems"] = 5,
			["100M Money"] = 6,
			["3 Spins"] = 7,
			["Random Pickaxe"] = 8,
			["265 Gems"] = 9,
			["10 Spins"] = 10
		}
		getgenv().selectedMethodSpin = map[Option] or nil
		if getgenv().selectedMethodSpin then
			SendNotification("GG hub", "Selected: " .. Option, 2)
		else
			SendNotification("GG hub", "Invalid selection!", 2)
		end
	end,
})

local ToggleAutoSpinWheelWithMethod = Shop:CreateToggle({
	Name = "Auto Spin Wheel With Method",
	CurrentValue = false,
	Flag = "Auto Spin Wheel With Method",
	Callback = function(Value)
		getgenv().autoSpin = Value

		if Value then
			if not getgenv().selectedMethodSpin then
				SendNotification("GG hub", "Select a method first!", 3)
				return
			end
			task.spawn(function()
				while getgenv().autoSpin do
					task.wait(0.4)
					pcall(function()
						game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("SpinPrizeEvent"):FireServer(getgenv().selectedMethodSpin)
					end)
				end
			end)
		end
	end,
})

Rayfield:LoadConfiguration()
