local games = {
    [155615604] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhubPrisonLife", -- Prison Life
    [221718525] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhubNinjaTycoon" -- Ninja Tycoon
}

local placeId = game.PlaceId
local scriptUrl = games[placeId]

if scriptUrl then
    loadstring(game:HttpGet(scriptUrl))()
else
    game.Players.LocalPlayer:Kick("Game not supported!")
end
