local GameScripts = {
    -- Prison Life
    [155615604] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhub_PrisonLife",
    -- Ninja Tycoon
    [221718525] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhub_NinjaTycoon",
    -- Just A Single Obby
    [94618981088971] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhub_JustASingleObby",
-- oMega Obby
[800308467] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhub_OMegaObby",
-- Tennis Clash
[85056532479796] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhub_TennisClash"
}
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local placeId = game.PlaceId
local scriptUrl = GameScripts[placeId]

local function SafeLoad(url)
    local success, response = pcall(function()
        return game:HttpGet(url)
    end)

    if success and response and response ~= "" then
        local loadSuccess, err = pcall(function()
            loadstring(response)()
        end)

        if not loadSuccess then
            warn("[GGHub] Error executing script:", err)
            LocalPlayer:Kick("Error loading Script: (" .. tostring(err) .. ")")
        end
    else
        warn("[GGHub] Failed to get URL from: ", url)
        LocalPlayer:Kick("Failed to get URL: " .. url)
    end
end

if scriptUrl then
    SafeLoad(scriptUrl)
else
    LocalPlayer:Kick("Game not supported!")
end
