	Name = "Instant Steal",
	CurrentValue = false,
	Flag = "Instant Steal",
	Callback = function(Value)
getgenv().instantSteal = Value

PPS.PromptButtonHoldBegan:Connect(function(prompt)
	if instantSteal and prompt.ActionText == "Take" then
		fireproximityprompt(prompt)
	end
end)
end,
})

local ToggleAutoLockBase = Main:CreateToggle({
	Name = "Auto Lock Base",
	CurrentValue = false,
	Flag = "Auto Lock Base",
	Callback = function(Value)
		getgenv().autoLockBase = Value

		task.spawn(function()
			while getgenv().autoLockBase do
				task.wait(0.1)
				pcall(function()
					if not plrBase or not plrBase:FindFirstChild("LockgateButton") then return end
					local button = plrBase.LockgateButton:FindFirstChild("Button")
					if not button then return end
					local timer = button:FindFirstChild("LockTimer") and button.LockTimer:FindFirstChild("Frame") and button.LockTimer.Frame:FindFirstChild("TextLabel")
					if not timer then return end

					if timer.Text == "0s" or timer.Text == "Lock base" then
						local args = {
	{
		workspace:WaitForChild("Bases"):WaitForChild("2"):WaitForChild("LockgateButton"):WaitForChild("Button"),
		"\026"
	}
}
game:GetService("ReplicatedStorage"):WaitForChild("ncxyzero_bridgenet2-fork@1.1.5"):WaitForChild("dataRemoteEvent"):FireServer(unpack(args))
					end
				end)
			end
		end)
	end,
})

Main:CreateToggle({
	Name = "Auto Attack",
	CurrentValue = false,
	Flag = "Auto Attack",
	Callback = function(Value)
		getgenv().autoAttack = Value
		
		task.spawn(function()
			while getgenv().autoAttack do
				task.wait(0.1)
				pcall(function()
					if not char then return end
					local tool = char:FindFirstChildOfClass("Tool") or plr.Backpack:FindFirstChild(originalTool)
					if not tool then return end
					if tool.Parent ~= char then
						tool.Parent = char
						task.wait(0.1)
					end
					if getgenv().autoAttack then
						tool:Activate()
					end
				end)
			end
		end)
	end,
})

local ToggleAutoCollectMoney = Main:CreateToggle({
	Name = "Auto Collect Money",
	CurrentValue = false,
	Flag = "Auto Collect Money",
	Callback = function(Value)
		getgenv().autoCollect = Value
		
		task.spawn(function()
			while getgenv().autoCollect do
				task.wait(getgenv().collectMoneyTime)
				pcall(function()
					if not plrBase or not plrBase:FindFirstChild("PlotSlots") then return end

for _,v in pairs(plrBase.PlotSlots:GetChildren()) do
if v:IsA("Model") and v:FindFirstChild("ClaimButton") then
if v.ClaimButton:FindFirstChild("Button") then
local args = {
	{
v.ClaimButton.Button,
		"\027"
	}
}
game:GetService("ReplicatedStorage"):WaitForChild("ncxyzero_bridgenet2-fork@1.1.5"):WaitForChild("dataRemoteEvent"):FireServer(unpack(args))
end
end
end
				end)
			end
		end)
	end,
})

