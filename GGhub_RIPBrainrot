local EspBrainrotColor = Color3.fromRGB(85,255,0)

Esp:CreateColorPicker({
	Name = "ESP Brainrot Color",
	Color = EspBrainrotColor,
	Callback = function(Value)
		EspBrainrotColor = Value
	end,
})

Esp:CreateToggle({
	Name = "Esp Brainrots From Bases",
	CurrentValue = false,
	Flag = "Esp Brainrots From Bases",
	Callback = function(Value)
		getgenv().autoEsp = Value
		task.spawn(function()
			while true do
				task.wait(0.3)
				local bases = workspace:FindFirstChild("Bases")
				if not bases then continue end

				for _,v in pairs(bases:GetChildren()) do
					local npcs = v:FindFirstChild("NPCs")
					if npcs then
						for _,npc in pairs(npcs:GetChildren()) do
							if not getgenv().autoEsp then
								if npc:FindFirstChild("GG_Highlight") then npc.GG_Highlight:Destroy() end
								if npc:FindFirstChild("GG_Billboard") then npc.GG_Billboard:Destroy() end
								for _,c in pairs(npc:GetDescendants()) do
									if c:IsA("BillboardGui") and c.Name=="GG_Billboard" then c:Destroy() end
								end
							end
						end
					end
				end

				if not getgenv().autoEsp then break end

				for _,v in pairs(bases:GetChildren()) do
					if v == plrBase then continue end
					local npcs = v:FindFirstChild("NPCs")
					if not npcs then continue end

					for _,npc in pairs(npcs:GetChildren()) do
						if npc:IsA("Model") and npc:GetAttribute("ConfigName") and not npc:FindFirstChild("GG_Highlight") then
							local adornee = npc.PrimaryPart or npc:FindFirstChildWhichIsA("BasePart")
							if not adornee then
								for _,p in ipairs(npc:GetChildren()) do
									if p:IsA("BasePart") then adornee = p break end
								end
							end
							if not adornee then continue end

							local h = Instance.new("Highlight")
							h.Name = "GG_Highlight"
							h.FillTransparency = 0.3
							h.OutlineTransparency = 0
							h.FillColor = EspBrainrotColor
							h.Adornee = npc
							h.Parent = npc

							local g = Instance.new("BillboardGui")
							g.Name = "GG_Billboard"
							g.AlwaysOnTop = true
							g.MaxDistance = 5000
							g.Size = UDim2.new(0,120,0,25)
							g.StudsOffset = Vector3.new(0,6,0)
							g.ZIndexBehavior = Enum.ZIndexBehavior.Global
							g.Adornee = adornee
							g.Parent = adornee

							local l = Instance.new("TextLabel")
							l.Size = UDim2.new(1,0,1,0)
							l.BackgroundTransparency = 1
							l.Text = tostring(npc:GetAttribute("ConfigName") or "Brainrot")
							l.TextColor3 = EspBrainrotColor
							l.TextScaled = true
							l.Font = Enum.Font.FredokaOne
							l.Parent = g

							local s = Instance.new("UIStroke")
							s.Thickness = 2.8
							s.Color = Color3.fromRGB(0,0,0)
							s.Transparency = 0
							s.Parent = l
						end
					end
				end
			end
		end)
	end,
})

Esp:CreateToggle({
	Name = "Auto Esp Brainrots From Road",
	CurrentValue = false,
	Flag = "Auto Esp Brainrots From Road",
	Callback = function(Value)
		getgenv().autoEsp = Value
		task.spawn(function()
			while true do
				task.wait(0.3)
				local folder = workspace:FindFirstChild("BrainrotRunway")
				if not folder or not folder:FindFirstChild("BrainrotsWalking") then continue end

				for _,v in pairs(folder.BrainrotsWalking:GetChildren()) do
					if not getgenv().autoEsp then
						if v:FindFirstChild("GG_Highlight") then v.GG_Highlight:Destroy() end
						if v:FindFirstChild("GG_Billboard") then v.GG_Billboard:Destroy() end
						for _,c in pairs(v:GetDescendants()) do
							if c:IsA("BillboardGui") and c.Name=="GG_Billboard" then c:Destroy() end
						end
					end
				end

				if not getgenv().autoEsp then break end

				for _,v in pairs(folder.BrainrotsWalking:GetChildren()) do
					if v:IsA("Model") and v:GetAttribute("ConfigName") and not v:FindFirstChild("GG_Highlight") then
						local adornee = v.PrimaryPart or v:FindFirstChildWhichIsA("BasePart")
						if not adornee then
							for _,p in ipairs(v:GetChildren()) do
								if p:IsA("BasePart") then adornee = p break end
							end
						end
						if not adornee then continue end

						local h = Instance.new("Highlight")
						h.Name = "GG_Highlight"
						h.FillTransparency = 0.3
						h.OutlineTransparency = 0
						h.FillColor = EspBrainrotColor
						h.Adornee = v
						h.Parent = v

						local g = Instance.new("BillboardGui")
						g.Name = "GG_Billboard"
						g.AlwaysOnTop = true
						g.MaxDistance = 5000
						g.Size = UDim2.new(0,120,0,25)
						g.StudsOffset = Vector3.new(0,6,0)
						g.ZIndexBehavior = Enum.ZIndexBehavior.Global
						g.Adornee = adornee
						g.Parent = adornee

						local l = Instance.new("TextLabel")
						l.Size = UDim2.new(1,0,1,0)
						l.BackgroundTransparency = 1
						l.Text = tostring(v:GetAttribute("ConfigName") or "Brainrot")
						l.TextColor3 = EspBrainrotColor
						l.TextScaled = true
						l.Font = Enum.Font.FredokaOne
						l.Parent = g

						local s = Instance.new("UIStroke")
						s.Thickness = 2.8
						s.Color = Color3.fromRGB(0,0,0)
						s.Transparency = 0
						s.Parent = l
					end
				end
			end
		end)
	end,
})

local ButtonTeleportToYouBase = Teleports:CreateButton({
	Name = "Teleport To You Base",
	Callback = function()
	hrp.CFrame = plrBase.PlotTeritory.CFrame + Vector3.new(0, 4.5, 0)
	end,
})

local function GetBases()
    local bases = {}
    for _, v in ipairs(game.Workspace.Bases:GetChildren()) do
        if v:IsA("Model") then table.insert(bases, v.Name) end
    end
    if #bases == 0 then table.insert(bases, nil) end
    return bases
end
local DropdownSelectBaseToTeleport = Teleports:CreateDropdown({
	Name = "Select Base To Teleport",
	Options = GetBases(),
	CurrentOption = nil,
	Flag = "Select Base To Teleport",
	Callback = function(Option)
	if type(Option) == "table" then Option = Option[1] end
        getgenv().currentSelectBase = Option or nil
	end,
})

local ButtonTeleportToSelectBase = Teleports:CreateButton({
	Name = "Teleport To Select Base",
	Callback = function()
	if getgenv().currentSelectBase ~= nil then
	for _, v in ipairs(game.Workspace.Bases:GetChildren()) do
        if v:IsA("Model") and v.Name == currentSelectBase then
	hrp.CFrame = v.PlotTeritory.CFrame + Vector3.new(0, 3.5, 0)
	end
	end
	end
	end,
})

local ButtonTeleportToStartBrainrotRunway = Teleports:CreateButton({
	Name = "Teleport To Start Brainrot Runway",
	Callback = function()
	hrp.CFrame = workspace.BrainrotRunway.Start.CFrame
	end,
})

local ButtonTeleportToEndBrainrotRunway = Teleports:CreateButton({
	Name = "Teleport To End Brainrot Runway",
	Callback = function()
	hrp.CFrame = workspace.BrainrotRunway.End.CFrame
	end,
})

local GetAllTools = Visuals:CreateButton({
	Name = "Get All Tools",
	Callback = function()
	for _,v in pairs(game:GetService("ReplicatedStorage").shared.model.Tools:GetChildren()) do
if v:IsA("Tool") then
v:Clone().Parent = plr.Backpack
end
end
	end,
})

local ButtonDestroyAllBasesWalls = Visuals:CreateButton({
	Name = "Destroy All Bases Walls",
	Callback = function()
	for _,v in pairs(game.Workspace.Bases:GetDescendants()) do
if v.Name == "GateMainCFrame" and v.Parent.PlotTeritory:GetAttribute("Player") ~= plr.Name then
	v:Destroy()
	end
	if v.Name == "PlotDecor" and v.Parent.PlotTeritory:GetAttribute("Player") ~= plr.Name then
	v:Destroy()
	end
	if v.Name == "Gate" and v.Parent.PlotTeritory:GetAttribute("Player") ~= plr.Name then
	v:Destroy()
	end
	if v.Name == "CollectPart" and v.Parent.PlotTeritory:GetAttribute("Player") ~= plr.Name then
	v:Destroy()
	end
	end
	end,
})

Rayfield:LoadConfiguration()
