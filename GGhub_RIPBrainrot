local ToggleAutoLockBase = Main:CreateToggle({
	Name = "Auto Lock Base",
	CurrentValue = false,
	Flag = "Auto Lock Base",
	Callback = function(Value)
		getgenv().autoLockBase = Value

		task.spawn(function()
			while getgenv().autoLockBase do
				task.wait(0.001)
				pcall(function()
					if not plrBase or not plrBase:FindFirstChild("LockgateButton") then return end
					local button = plrBase.LockgateButton:FindFirstChild("Button")
					if not button then return end
					local timer = button:FindFirstChild("LockTimer") and button.LockTimer:FindFirstChild("Frame") and button.LockTimer.Frame:FindFirstChild("TextLabel")
					if not timer then return end

					if timer.Text == "Lock base" then
						local args = {
	{
		plrBase:WaitForChild("LockgateButton"):WaitForChild("Button"),
		"\026"
	}
}
game:GetService("ReplicatedStorage"):WaitForChild("ncxyzero_bridgenet2-fork@1.1.5"):WaitForChild("dataRemoteEvent"):FireServer(unpack(args))
					end
				end)
			end
		end)
	end,
})

Main:CreateToggle({
	Name = "Auto Attack",
	CurrentValue = false,
	Flag = "Auto Attack",
	Callback = function(Value)
		getgenv().autoAttack = Value
		
		task.spawn(function()
			while getgenv().autoAttack do
				task.wait(0.1)
				pcall(function()
					if not char then return end
					local tool = char:FindFirstChildOfClass("Tool") or plr.Backpack:FindFirstChild(originalTool)
					if not tool then return end
					if tool.Parent ~= char then
						tool.Parent = char
						task.wait(0.1)
					end
					if getgenv().autoAttack then
						tool:Activate()
					end
				end)
			end
		end)
	end,
})

local ToggleAntiRagdoll = Main:CreateToggle({
	Name = "Anti Ragdoll",
	CurrentValue = false,
	Flag = "Anti Ragdoll",
	Callback = function(Value)
	getgenv().antiRagdoll = Value
	
	task.spawn(function()
		while true do task.wait(.0001)
			if not antiRagdoll then return end
			hum.EvaluateStateMachine = true	
		end
	end)
end,
})

