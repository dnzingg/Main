local ToggleAutoOpenGifts = MainOthers:CreateToggle({
Name = "Auto Open Gifts",
CurrentValue = false,
Flag = "Auto Open Gifts",
Callback = function(Value)
getgenv().autoGift = Value

while autoGift do task.wait(.1)
for _,gift in pairs(workspace:GetChildren()) do
if gift.Name == "Gift" and gift:IsA("Model") then
if gift:FindFirstChild("RootPart") then
local rootPart = gift.RootPart
local oldPos = hrp.CFrame
hrp.CFrame = rootPart.CFrame
task.wait(.1)
fireproximityprompt(rootPart:FindFirstChildOfClass("ProximityPrompt"))
task.wait(.2)
hrp.CFrame = oldPos
end
end
end
end
end,
})

