	Name = "Instant Steal",
	CurrentValue = false,
	Flag = "Instant Steal",
	Callback = function(Value)
getgenv().instantSteal = Value

PPS.PromptButtonHoldBegan:Connect(function(prompt)
	if instantSteal and prompt.ActionText == "Take" or prompt.ActionText == "Steal" then
		fireproximityprompt(prompt)
	end
end)
end,
})

